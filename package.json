        "node-machine-id": "^1.1.12",
        "ora": "^6.1.2",
        "tar": "^6.1.11",
        "uuid": "^8.3.2",
        "validate-npm-package-name": "^4.0.0",
        "which-pm-runs": "^1.1.0",
        "yargs": "^17.5.1"
    },
    "bin": "lib/build/index.js",
